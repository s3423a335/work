有1、2、3、4個數字，能組成多少個互不相同且無重複數字的三位數？都是多少？

c
=========================================================
	int a,b,c;
    int d=0;
    for(a=1;a<5;a++) 
    	for(b=1;b<5;b++) 
    		for(c=1;c<5;c++) 
    			if ((a != b) && (a != c) && (b != c)){
    				d+=1;
    				printf("%d,%d,%d\n",a,b,c);
    			}
                     printf("總共有%d個",d);
}
 return 0;
==========================================================
c++
==========================================================
int a,b,c;
    int d=0;
    for(a=1;a<5;a++) 
    	for(b=1;b<5;b++) 
    		for(c=1;c<5;c++) 
    			if ((a != b) && (a != c) && (b != c)){
    				d+=1;
    				cout << a << "," << b << "," << c << "\n";
    			}
                     cout << "總共有" << d << "個";
		     
		     return 0;

